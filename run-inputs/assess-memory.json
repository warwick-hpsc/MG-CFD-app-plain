{
    "compile": {
        "compiler": "intel",
        "insn sets": [ "SSE42", "AVX2" ], 
        "base flags": "-DTIME -DPAPI -DOMP -DOMP_SCATTERS"
    },

    "run": {
        "data dirpath": "../MG-CFD-app-plain-datasets/m6wing",
        "num threads": [1, 2, 4, 6, 8, 12, 16, 20, 24], 
        "num repeats": 3,
        "mg cycles": 5,
        "unit walltime": 1.0,
        "min mesh multi": 2
    },

    "setup": {
        "jobs dir": "./run-jobs.assess-memory",
        "job scheduler": "?", 
        "partition": "?"
    }
}
